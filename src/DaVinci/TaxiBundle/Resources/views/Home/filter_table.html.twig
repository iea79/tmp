	<div class="table-over">
		<table class="serv responsive" cellspacing="1">
			<tr>
				<th class="tab-h1">Tip	<div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Vehicle type{% endtrans %}">?</div></th>
				<th class="daterange">Date/Time <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Select date and time{% endtrans %}">?</div></th>
				<th>Route <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Pick-up/drop off location{% endtrans %}">?</div></th>
				<th>Tips <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Increase your rating{% endtrans %}">?</div></th>
				<th>PM <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Payment method{% endtrans %}" >?</div></th>
				<th>Price <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Set your price (fare){% endtrans %}" >?</div></th>
				<th>Status <div class="questiontooltip" data-uk-tooltip="{pos:'top'}" title="{% trans %}Check bought deals{% endtrans %}" >?</div></th>
				{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %} <th class="tab-h2">Buy</th>{% endif %}
			</tr>
			<tr class="under-head">
				<td class="auto-tip">
				<div class="car">
                                    
					<button class="uk-button-link">
						<i class="mp-icon-taxi"></i>
						<i class="uk-icon-caret-down uk-text-dark"></i>
					</button>
					<select>
						<option>{{'Economy'|trans}}</option>
						<option>{{'Compact'|trans}}</option>
						<option>{{'Midsize'|trans}}</option>
						<option>{{'Standart'|trans}}</option>
						<option>{{'Full Size'|trans}}</option>
						<option>{{'Premium'|trans}}</option>
						<option>{{'Midsize SUV'|trans}}</option>
						<option>{{'Standart Convertible'|trans}}</option>
						<option>{{'Premium Pickup'|trans}}</option>
						<option>{{'Mini Van'|trans}}</option>
						<option>{{'Standart SUV'|trans}}</option>
						<option>{{'Standart SUV'|trans}}</option>
						<option>{{'Full Size Pickup'|trans}}</option>
						<option>{{'Full Size SUV'|trans}}</option>
						<option>{{'Standart Pickup'|trans}}</option>
						<option>{{'Full Size SUV'|trans}}</option>
						<option>{{'Special Van'|trans}}</option>
						<option>{{'Premium Van'|trans}}</option>
						<option>{{'Luxury'|trans}}</option>
						<option>{{'Premium Special'|trans}}</option>
						<option>{{'Premium SUV'|trans}}</option>
						<option>{{'Special'|trans}}</option>
					</select>
					
				</div>
					
				</td>
				<td class="daterange">
					<div id="widget">
						<div id="widgetField" class="uk-form-icon">
							<input type="text" name="datarange" maxlength="23" value="" placeholder="02/12/14-26/12/14"/>
							<a href="#">&#58896;</a>
						</div>
						<div id="widgetCalendar">
						</div>
					</div>
					
				</td>
				<td class="destinations_list">
					<div class="uk-form-icon">
						<input type="text" name="destination"  placeholder="for example, Moscow, Russia" />
						<i class="mp-icon-nord-star"></i>
					</div>
					
				</td>
				<td class="trust">
					<div class="ov-trust">
						<div class="uk-button uk-form-select select" data-uk-form-select="">
							<span >All</span>
							<i class="uk-icon-caret-down"></i>
							<select class="iconed-drop uk-text-primary">
								<option value="1">All</option>
								<option value="2">&#58901;</option>
								<option value="3">&#58901; &#58901;</option>
								<option value="4">&#58900;</option>
										
							</select>
						</div>
					</div>
					
					
				</td>
				<td>
					<button href="#" class="sort uk-button-link arr single-butt">
						
						<i class="mp-icon-arrow-up uk-text-dark"></i>
						<i class="mp-icon-arrow-down"></i>
					</button>
					
				</td>
				<td class="price">
					<div class="ov-lt short-cont">
						<button href="#"  class="sort uk-button-link aligment arr">
							
							<i class="mp-icon-arrow-up uk-text-dark"></i>
							<i class="mp-icon-arrow-down"></i>
						</button>
						<div class="uk-button uk-form-select select aligment fit-in" data-uk-form-select="">
						<span>$10-$20</span>
						 <i class="uk-icon-caret-down"></i>
							<select>
								<option value="1">$10-$50</option>
								<option value="2">$50-$100</option>
								<option value="3">$100-$150</option>
								<option value="3">$150-$200</option>
								<option value="4">$200+</option>
							</select>
						</div>
					</div>
					
				</td>
				<td class="status">
					
						<div class="uk-button uk-form-select select " data-uk-form-select="">
                    <span>All</span>
                     <i class="uk-icon-caret-down"></i>
                        <select >
                            <option value="1">All</option>
                            <option value="2">Open</option>
                            <option value="3">Pending</option>
                            <option value="4">Closed</option>
                            <option value="4">Canceled</option>
                            <option value="4">All happy</option>
                        </select>
					</div>
					<div class="questiontooltip">
                   <a class="tooltip" href="#"><span class="que">?</span><span class="tooltip">Mobile internet</span></a>
                  </div>	
				</td>
				{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
				<td>
					<div class="ov-lt fixed-column">
						
					<div class="uk-button uk-form-select select aligment bascket-opt" data-uk-form-select="">
						<span>Other</span>
						 <i class="uk-icon-caret-down"></i>
							<select>
								<option value="1">Short sale</option>
								<option value="3">Other</option>
								
							</select>
					</div>
						<button class="cancell-filters uk-button-link  aligment" >	
					
						<i class="mp-icon-closed uk-text-danger"></i>
						</button>
					</div>
					
				</td>
				{% endif %}
			</tr>
	
			<tr class="table-cont">
				<td><span class="blue-bg">1</span></td>
				<td>2 <strong>MAR</strong> 14<br/> 18:45</td>
				<td><a data-uk-modal="{target:'#route'}">{{ 'Russia'|trans }},{{ ' Moscow'|trans }}<br>{{ 'From'|trans }}: {{ 'aeroport Domodedovo'|trans }} - {{ 'To'|trans }}:  - {{ 'center'|trans }}</a></td>
				<td><i class="mp-icon-trust uk-text-primary"></i></td>
				<td><span class="escro">escrow</span></td>
				<td>$783.85</td>
				<td>open</td>
