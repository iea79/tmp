<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}TAXI MY PRICE{% endblock %}</title>
		{% block stylesheets %}
            {% stylesheets 	'bundles/davincitaxi/css/uikit.css'
				'bundles/davincitaxi/css/addons/uikit.addons.css'
                'bundles/davincitaxi/css/flick/jquery-ui-1.10.4.custom.min.css' filter='cssrewrite' 
				'bundles/davincitaxi/css/styles.css' %}
				<link media="all" rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
		{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('bundles/davincitaxi/images/favicon.ico') }}" />
    </head>

<body>
	<div class="registerfix"></div>
	<div class="wrapper {% block theme %}default{% endblock %}">
		<div class="wrapper-in">
			{% block header %}
			<div id="header" class="wrapper-content">
				<div class="content">
					
					<table>
						<tr>
							<td>
								<strong class="logo"><a href="{{ url('da_vinci_taxi_homepage') }}">TAXI MY PRICE</a></strong>
							</td>
							<td>
							</td>
							<td>
								<nav id="main-nav">
									<ul class="uk-subnav uk-subnav-pill">
										<li><a href="#">About</a></li>
										<li><a href="#">Help</a></li>
										<li>
											<a href="#"><img src="{{ asset('bundles/davincitaxi/images/ico01.png') }}" alt="Passenger" width="27" height="27"></a>
											<a href="#">Profit</a>
											<a href="#"><img src="{{ asset('bundles/davincitaxi/images/ico02.png') }}" alt="Driver" width="27" height="27"></a>
										</li>
									</ul>
								</nav>
							</td>
							<td>
								<div class="typevis"><a class="book" href="#">Book</a><a href="#">Store</a><div class="razd"></div></div>
							</td>
							<td class="lastvis">
								<ul class="uk-subnav uk-subnav-pill uk-button-group signin">
									<li data-uk-dropdown="{mode:'click'}" class="">
										<a href="#" class="uk-button uk-button-link">Hello, Sign in <i class="uk-icon-caret-down"></i></a>
										<div class="uk-dropdown uk-dropdown-medium">
											<ul class="uk-nav uk-nav-dropdown">
												<li><a href="{{ url('fos_user_security_login') }}" class="uk-button uk-button-link">Register</a></li>
												<li><a class="uk-button uk-button-success" href="#">Log in</a></li>
											</ul>
										</div>
									</li>
								</ul>
							</td>
							<td>
								<a href="javascript://" class="uk-navbar-toggle" data-uk-offcanvas="{target:'#offcanvas-nav'}"></a><strong></strong>
							</td>
						</tr>
					</table>
				</div>
			</div>
			{% endblock %}
			{% block body %}{% endblock %}
			{% block javascripts %}
					<!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->

					{% javascripts
						'@DaVinciTaxiBundle/Resources/public/js/jquery.js'
						'@DaVinciTaxiBundle/Resources/public/js/jquery.mousewheel.js'
						'@DaVinciTaxiBundle/Resources/public/js/jquery-ui-1.10.4.custom.min.js'
						'@DaVinciTaxiBundle/Resources/public/js/uikit.min.js'
						'@DaVinciTaxiBundle/Resources/public/js/timepicker.js'
						'@DaVinciTaxiBundle/Resources/public/js/custom-form-elements.js'
						'@DaVinciTaxiBundle/Resources/public/js/jquery.form.js'
						'@DaVinciTaxiBundle/Resources/public/js/jquery.validate.js'
						'@DaVinciTaxiBundle/Resources/public/js/all.js' %}
					<script src="{{ asset_url }}"></script>
					{% endjavascripts %}
			{% endblock %}
				
			<div id="offcanvas-nav" class="uk-offcanvas">
				<div class="uk-offcanvas-bar uk-offcanvas-bar-flip">
					<form class="uk-search in-offcanvas" data-uk-search="{source:'json/_searchautocomplete.json'}">
						<input class="uk-search-field" type="search" placeholder="search">
						<button class="uk-search-close" type="reset"></button>
					</form>
					<ul class="uk-nav uk-nav-offcanvas uk-nav-parent-icon" data-uk-nav="{multiple:false}">
						<li><a href="index.html">Index</a></li>
						<li><a href="registration.html">Register</a></li>
						<li><a href="registration-complete.html">Register Complete</a></li>
						<li class="uk-active"><a href="">Active</a></li>
						<li class="uk-parent">
							<a href="#">Parent</a>
							<ul class="uk-nav-sub">
								<li><a href="">Sub item</a></li>
								<li><a href="">Sub item</a>
									<ul>
										<li><a href="">Sub item</a></li>
										<li><a href="">Sub item</a></li>
									</ul>
								</li>
							</ul>
						</li>
						<li class="uk-nav-header offcanvas-footer">Footer</li>
						<li class="uk-parent offcanvas-footer">
							<a href="#">About MMA Starter</a>
							<ul class="uk-nav-sub">
								<li><a href="#">About Us</a></li>
								<li><a href="#">Contact Us</a></li>
								<li><a href="#">MMA Starter Careers</a></li>
								<li><a href="#">MMA Starter Store</a></li>
								<li><a href="#">Legal</a></li>
								<li><a href="#">Privacy Policy</a></li>
							</ul>
						</li>
						<li class="uk-parent offcanvas-footer">
							<a href="#">Messenger</a>
							<ul class="uk-list">
								<li><a href="#">Messages</a></li>
								<li><a href="#">Dialogs</a></li>
								<li><a href="#">Notification</a></li>
							</ul>
						</li>
						<li class="uk-parent offcanvas-footer">
							<a href="#">My Offices</a>
							<ul class="uk-list">
								<li><a href="#">Office Fighter</a></li>
								<li><a href="#">Office Agent</a></li>
								<li><a href="#">Office Promoter</a></li>
								<li><a href="#">Office Promotion</a></li>
								<li><a href="#">My Offices</a></li>
								<li><a href="#">All Offices</a></li>
							</ul>
						</li>
						<li class="uk-parent offcanvas-footer">
							<a href="#">My Account</a>
							<ul class="uk-list">
								<li><a href="#">Profile</a></li>
								<li><a href="#">Settings</a></li>
								<li><a href="#">Create Account</a></li>
							</ul>
						</li>
						<li class="uk-nav-divider offcanvas-footer"></li>
						<li class="offcanvas-footer">
							<a href="#"><i class="uk-icon-youtube-play"></i> YouTube</a>
							<a href="#"><i class="uk-icon-twitter"></i> Twitter</a>
							<a href="#"><i class="uk-icon-facebook-square"></i> Facebook</a>
							<a href="#"><i class="uk-icon-vk"></i> VK</a>
							<a href="#"><i class="uk-icon-instagram"></i> Instagram</a>
						</li>
					</ul>
				</div>
			</div>

		</div>
		{% block footer %}
			 <footer id="footer" class="wrapper-content">
				<div class="footer-block">
					<div class="content">
						<nav class="add-nav">
							<ul class="uk-subnav uk-subnav-pill">
								<li><a href="#">Info</a></li>
								<li><a href="#">Privacy Policy</a></li>
							</ul>
						</nav>

						<ul class="uk-subnav uk-subnav-pill social">
							<li><a class="youtube" href="#" title="youtube">youtube</a></li>
							<li><a class="twitter" href="#">twitter</a></li>
							<li><a class="facebook" href="#">facebook</a></li>
							<li><a class="google" href="#" title="google+">google+</a></li>
							<li><a class="linkedin" href="#">linkedin</a></li>
						</ul>
						<div class="clear"></div>
					</div>
				</div>
				<div class="copy"><p class="w1 content">&copy; {{ "now"|date("Y") }}  TaxiMyPrice</p></div>
			</footer>
		{% endblock %}
	</div>
</body>

</html>

