<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        {{ sonata_seo_title() }}
        {{ sonata_seo_metadatas() }}
        {% block stylesheets %}
            {% stylesheets 'bundles/davincitaxi/css/uikit.css'
					'bundles/davincitaxi/css/addons/uikit.addons.css'
					'bundles/davincitaxi/css/flick/jquery-ui-1.10.4.custom.min.css' 
					'bundles/davincitaxi/css/styles.css' 
					'bundles/davincitaxi/css/lena.css' 
                    'bundles/davincitaxi/js/intl-tel-input-master/css/intlTelInput.css' filter='cssrewrite' 
            %}

            <link media="all" rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('bundles/davincitaxi/images/favicon.ico') }}" />
    </head>
    <body class="{% block theme %}default{% endblock %}">
        <div class="wrap-content">
            {% block header %}

                <header  class="header-wrapper">
                    <div class="header-top">
                        <div class="header-top-in">
                            <nav class="topmenu">
                                <ul class="uk-subnav uk-subnav-pill">
                                    <li>
                                        <a href="{{ url('da_vinci_taxi_homepage') }}">
                                            <img src="{{ asset('bundles/davincitaxi/images/logo.png') }}" alt="{{ 'Taxi My Price'|trans}}" />
                                        </a>
                                    </li>
                                    <li></li>
                                    <li class="nest">
                                        <a href="#" class="texst-item">{{ 'About'|trans }}</a>
                                        <a href="#" class="texst-item">{{ 'Help'|trans }}</a>
                                        <a href="#" class="nest-item">
                                            <span class="uk-button-danger figur-item"><i class="mp-icon-person"></i></span>
                                            <span>{{ 'Profit'|trans }}</span>
                                            <span class="uk-button-primary figur-item"><i class="mp-icon-taxi"></i></span>
                                        </a>
                                    </li>
                                    <li class="empty-middle"></li>

                                    <li>
                                        <div class="book-store">
                                            <a class="book uk-button-danger" href="#">{{ 'Book'|trans }}</a>
                                            <a href="#" class="uk-button-primary">{{ 'Store'|trans }}</a>
                                            <div class="thumb">
                                                <i class="mp-icon-good"></i>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                            <div data-uk-dropdown="" class="uk-button-dropdown">
                                                <button class="uk-button  uk-button-link autorized">
                                                    <span class="caption">My account</span> <i class="uk-icon-caret-down uk-margin-small-left"></i><br/><span class="ownship">passenger</span>
                                                </button>
                                                <div class="uk-dropdown uk-dropdown-width-3 shadow-main">
                                                    <div class="myaccount-drop">
                                                        <ul class="uk-nav uk-nav-dropdown">
                                                            <li class="uk-nav-header">My account</li>
                                                            <li><a href=""><i class="mp-icon-person"></i> My profile</a></li>
                                                            <li><a href=""><i class="uk-icon-envelope"></i> Messages</a></li>
                                                            <li><a href=""><i class="uk-icon-sign-out"></i> Log Out</a></li>
                                                        </ul>
                                                    </div>
                                                    <div class="myoffice-drop">
                                                        <ul class="uk-nav uk-nav-dropdown">
                                                            <li class="uk-nav-header">My office</li>
                                                            <li><span>Passenger<br /> Independent driver</span></li>
                                                        </ul>
                                                        <a href="{{ url('fos_user_registration_confirmed') }}"><button class="uk-button uk-button-success">Get new office</button></a>
                                                    </div>
                                                    <div class="myid-drop">
                                                        <div class="myid-drop-top">
                                                            <div class="img">
                                                                <img src="/bundles/davincitaxi/images/profile-photo2x.png" alt="" />
                                                            </div>
                                                            <ul class="uk-nav uk-nav-dropdown">
                                                                <li>
                                                                    <div class="clientid">My ID: <span class="data">76877960596623</span></div>
                                                                </li>
                                                                <li>
                                                                    <div class="clientid">Name: <span class="data">{{ app.user.firstname }} {{ app.user.lastname }}</span></div>
                                                                </li>
                                                                <li>
                                                                    <div class="clientid">My balance: <span class="data">$0.00</span></div>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                        <ul class="uk-grid  menu-bord">
                                                            <li><a href="">$Add</a></li>
                                                            <li><a href="">$Withdraw</a></li>
                                                            <li><a href="">$Transfer</a></li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        {% else %}
                                            <button class="uk-button  uk-button-link nonautorized">
                                                <a href="{{ url('fos_user_security_login') }}" class="uk-button uk-button-link">{{ 'Hello, Sign in'|trans }}</a>
                                            </button>
                                        {% endif %}

                                    </li>
                                </ul>
                            </nav>

                            <a href="javascript://" class="uk-navbar-toggle" data-uk-offcanvas="{target:'#offcanvas-nav'}"></a>
                        </div>
                    </div>
                    {% block breadcrumbs %}
                        <div class="header-bottom">
                            <nav class="header-bottom-in">
                                {{ knp_menu_render('breadcrumb',{'currentAsLink':false} ) }}
                                <ul class="uk-subnav uk-subnav-pill lang">
                                    <li data-uk-dropdown{#="{mode:'click'}" class=""#}>
                                        <a href="#" class="uk-button uk-button-link">

                                            {% if app.request.locale =='en' %} Eng {%elseif app.request.locale =='ru'%} Rus {% endif %}

                                            <i class="uk-icon-caret-down"></i></a>
                                        <div class="uk-dropdown uk-dropdown-small">
                                            <ul class="uk-nav uk-nav-dropdown">
                                                <li><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'en'})) }}">Eng</a></li>
                                            </ul>
                                        </div>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    {% endblock %}
                </header>
            {% endblock %}
			{% block title %}
			{% endblock %}
            <div class="main-content">
                {% block body %}
                {% endblock %}


            </div>

            <div id="offcanvas-nav" class="uk-offcanvas">
                <div class="uk-offcanvas-bar uk-offcanvas-bar-flip">
                    <ul class="uk-nav uk-nav-offcanvas uk-nav-parent-icon" data-uk-nav="{multiple:false}">
                        <li><a href="index.html"class="uk-button">
                                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %} 
                                    {{ 'Log In'|trans}} <i class="uk-icon-sign-in"></i>
                                {% else %} 
                                    {{ 'Sing Up'|trans}}
                                {% endif %}
                            </a></li>
                        <li><a href="index.html">Home <i class="uk-icon-home"></i></a></li>
                        <li class="uk-parent"><a href="#">Your passanger</a>
                            <ul class="uk-nav-sub">
                                <li><a href="">Book</a></li>
                                <li><a href="">Profit</a></li>
                                <li><a href="">Help</a></li>

                            </ul>
                        </li>
                        <li class="uk-parent"><a href="#">Your driver</a>
                            <ul class="uk-nav-sub">
                                <li><a href="">TMP market</a></li>
                                <li><a href="">Profit</a></li>
                                <li><a href="">Help</a></li>                   

                            </ul>
                        </li>
                        <li class="uk-parent"><a href="#">Store</a>
                            <ul class="uk-nav-sub">
                                <li><a href="">For passanger</a></li>
                                <li><a href="">For driver</a></li>
                                <li><a href="">General</a></li>
                                <li><a href="">Help</a></li>                   

                            </ul>
                        </li>
                        <li class="uk-active"><a href="">About TMP</a></li>
                        <li class="uk-active"><a href="">TMP agreements</a></li>
                        <li class="uk-active"><a href="">Media</a></li>
                        <li class="uk-active"><a href="">Log out <i class="mp-icon-logout"></i></a></li>
                    </ul>
                </div>
            </div>

        </div>

        {% block footer %}

            <footer>
                <div class="footer-top">
                    <div class="footer-in">
                        <nav class="add-nav">
                            <ul class="uk-subnav uk-subnav-pill uk-list">
                                <li><a href="#">{{ 'Privacy policy'|trans }}</a></li>
                                <li><a href="#">{{ 'Blog'|trans }}</a></li>
                            </ul>
                        </nav>

                        <ul class="uk-subnav uk-subnav-pill social uk-list">

                            <li><a class="twitter" href="#"><i class="uk-icon-twitter"></i></a></li>
                            <li><a class="facebook" href="#"><i class="uk-icon-facebook"></i></a></li>
                            <li><a class="google" href="#" title="google+"><i class="uk-icon-google-plus"></i></a></li>
                            <li><a class="linkedin" href="#"><i class="uk-icon-linkedin"></i></a></li>
                        </ul>
                        <div class="clear"></div>
                    </div>
                </div>
                <div class="footer-bottom">
                    <p>&copy; {{ "now"|date("Y") }}  TaxiMyPrice</p>
                </div>

            </footer>
        {% endblock %}
        {% block javascripts %}

            <script>
                var liphone_utils_path = "{{ asset('bundles/davincitaxi/js/intl-tel-input-master/lib/libphonenumber/build/utils.js') }}";
            </script>
            <!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
            {% javascripts
							'@DaVinciTaxiBundle/Resources/public/js/jquery.js'
							'@DaVinciTaxiBundle/Resources/public/js/uikit.min.js'
							'@DaVinciTaxiBundle/Resources/public/js/addons/form-select.js'
							'@DaVinciTaxiBundle/Resources/public/js/addons/form-password.min.js'
							'@DaVinciTaxiBundle/Resources/public/js/timepicker.js'
							'@DaVinciTaxiBundle/Resources/public/js/jquery.spinner.min.js'
							'@DaVinciTaxiBundle/Resources/public/js/jquery.validate.js'
							'@DaVinciTaxiBundle/Resources/public/js/custom-form-elements.js'
							'@DaVinciTaxiBundle/Resources/public/js/jquery.form.js'
							'@DaVinciTaxiBundle/Resources/public/js/datarange.js'
							'@DaVinciTaxiBundle/Resources/public/js/chosen.jquery.js'
							'@DaVinciTaxiBundle/Resources/public/js/chosenImage.jquery.js'
							'@DaVinciTaxiBundle/Resources/public/js/intl-tel-input-master/js/intlTelInput.js'
                            '@DaVinciTaxiBundle/Resources/public/js/register_form.js'
							'@DaVinciTaxiBundle/Resources/public/js/all.js'
            %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>